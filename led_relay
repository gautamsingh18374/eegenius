void setup() {
  // put your setup code here, to run once:
  pinMode(0,OUTPUT);
  pinMode(1,OUTPUT);
  pinMode(2,OUTPUT);
  pinMode(3,OUTPUT);
  pinMode(4,OUTPUT);
  pinMode(5,OUTPUT);
  pinMode(6,OUTPUT);
  pinMode(7,OUTPUT);
}

void loop() {
  int sensorValue = analogRead(A0);
  Serial.println(sensorValue);
  delay(1);
  digitalWrite(0,HIGH);
  digitalWrite(1,HIGH);
  digitalWrite(2,HIGH);
  digitalWrite(3,HIGH);
  digitalWrite(4,HIGH);
  digitalWrite(5,HIGH);
  digitalWrite(6,HIGH);
  digitalWrite(7,HIGH);
  delay(sensorValue);
  digitalWrite(0,LOW);
  delay(sensorValue);
  digitalWrite(1,LOW);
  delay(sensorValue);
  digitalWrite(2,LOW);
  delay(sensorValue);
  digitalWrite(3,LOW);
  delay(sensorValue);
  digitalWrite(4,LOW);
  delay(sensorValue);
  digitalWrite(5,LOW);
  delay(sensorValue);
  digitalWrite(6,LOW);
  delay(sensorValue);
  digitalWrite(7,LOW);
  delay(sensorValue);
}
